program T3;
var
    a: integer;

function EhPar(num: integer): boolean;
begin
    EhPar := (num div 2) * 2 = num;
end;

procedure ImprimirResultado(valor: integer; eh_par_flag: boolean);
begin
    if eh_par_flag then
        write(valor, true)
    else
        write(valor, false);
end;

begin
    read(a);
    ImprimirResultado(a, EhPar(a));
end.
